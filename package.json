{
  "name": "ingest-iq",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "engines": {
    "node": ">=18"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "packageManager": "yarn@4.9.1",
  "devDependencies": {
    "@repo/eslint-config": "workspace:packages/eslint-config",
    "eslint": "^8.57.0",
    "prettier": "^2.8.8",
    "turbo": "^2.5.3"
  }
}
